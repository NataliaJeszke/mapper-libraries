{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/main/getxml.jsx", "src/app/api/route.ts", "src/app/containers/handleUserFile/handleUserFile.jsx", "src/app/components/tag.jsx", "src/app/main/page.jsx", "src/app/containers/mapperWindow/mapperWindow.jsx", "src/app/containers/dropdownList/dropdownList.jsx", "src/app/utils/createObjectSchema.js"],
  "exclude": ["node_modules"]
}
